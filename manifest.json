{
    "name": "Dark SBHS Student Portal",
    "description": "Allows users of the SBHS Student Portal to switch to a dark theme.",
    "version": "1.0",
    "manifest_version": 3,
    "content_scripts": [
        {
            "matches": ["https://student.sbhs.net.au/*"],
            "css": ["dark.css"]
        }
    ],
    "web_accessible_resources": [
        {
            "resources": ["assets/logo-portal-student-white.png"],
            "matches": ["https://student.sbhs.net.au/*"]
        }
    ]
}
